{
  "extends": ["next/core-web-vitals", "airbnb-base", "prettier"],
  "rules": {
    "no-unused-vars": 1,
    "no-console": 1,
    "indent": ["error", 4],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true
      }
    ]
  },
  "plugins": ["prettier"]
}
